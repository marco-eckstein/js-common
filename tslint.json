{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "array-type": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "max-classes-per-file": false, // TODO
        "new-parens": false // TODO: False positives? Investigate.
    }
}
